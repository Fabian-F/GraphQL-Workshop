query azubi {
  azubi(id: 1) {
    name
    lastname
  }
}

query azubis {
  azubis {
    id
    name
    lastname
  }
}

mutation addAzubi {
  addAzubi(id: 4, name: "Alexander", lastname: "Klein")
}

mutation updateAzubi {
  updateAzubi(id: 2 name: "Test" lastname: "Test")
}

mutation removeAzubi {
  removeAzubi(id: 5)
}